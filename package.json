{
	"name": "action-able",
	"version": "0.1.0",
	"description": "A \"to-do\" MERN-stack web application with login/auth features and a React-based UI\"",
	"main": "index.js",
	"engines": {
		"node": "14.x"
	},
	"scripts": {
		"start": "node server",
		"build": "cd react-ui/ && npm install && npm run build",
		"devserver": "nodemon index.js",
		"devclient": "npm start --prefix react-ui",
		"dev": "concurrently \"npm run devserver\" \"npm run devclient\""
	},
	"prettier": "mj-pa",
	"cacheDirectories": [
		"node_modules",
		"react-ui/node_modules"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/killshot13/actionable.git"
	},
	"keywords": [
		"MERN",
		"mongo",
		"express",
		"create-react-app",
		"heroku",
		"node",
		"react",
		"javascript",
		"mongoose"
	],
	"author": "killshot13",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/killshot13/actionable/issues"
	},
	"homepage": "https://github.com/killshot13/actionable#readme",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"cluster": "^0.7.7",
		"concurrently": "^6.0.0",
		"debug": "^4.3.2",
		"dotenv": "^16.0.0",
		"express": "^4.17.1",
		"is-empty": "^1.2.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^6.4.6",
		"os": "^0.1.1",
		"passport": "^0.5.2",
		"passport-jwt": "^4.0.0",
		"path": "^0.12.7",
		"validator": "^13.7.0"
	},
	"devDependencies": {
		"mj-pa": "^1.3.1",
		"prettier": "^2.2.1",
		"nodemon": "^2.0.7"
	}
}
